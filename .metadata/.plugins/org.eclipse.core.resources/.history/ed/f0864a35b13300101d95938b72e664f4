package model;

public class User {
    private String name;
    private String passwordHash;

    // コンストラクタ
    public User(String name, String passwordHash) {
        this.name = name;
        this.passwordHash = passwordHash;
    }

    // ゲッター
    public String getName() {
        return name;
    }

    public String getPasswordHash() {
        return passwordHash;
    }
}
